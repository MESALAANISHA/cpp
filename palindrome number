//program to check for palindrome

#include <bits/stdc++.h>
using namespace std;
int main(){
	int n;
	cin>>n;
	int rev=0,rem,temp;
	temp=n;
	while(temp!=0){
		rem=temp%10;
		rev=rev*10+rem;
		temp=temp/10;
	}
		if(n==rev){
			cout<<n <<" is palindrone"<<"\n";
		}
		else{
			cout<<n <<" is not a palindrome";
		}
	}
